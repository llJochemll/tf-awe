    "@js-joda/core": "^5.5.2",
    "@koa/router": "^12.0.0",
    "@types/node-schedule": "^2.1.0",
    "discord.js": "^14.6.0",
    "discordx": "^11.4.0",
    "jsdom": "^20.0.1",
    "koa": "^2.13.4",
    "lowdb": "^3.0.0",
    "node-cache": "^5.1.2",
    "node-fetch": "^3.2.10",
    "node-schedule": "^2.1.0",
    "reflect-metadata": "^0.1.13",
    "unitaf": "*"
  },
  "devDependencies": {
    "@discordx/changelog": "^2.11.5",
    "@types/jsdom": "^20.0.0",
    "@types/koa": "^2.13.5",
    "dotenv": "^16.0.3",
    "nodemon": "^2.0.20",
    "pm2": "^5.2.2",
    "prettier": "^2.8.6",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
