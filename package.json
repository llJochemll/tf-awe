{
  "name": "tf-awe",
  "version": "1.0.0",
  "description": "Bot for TF-AWE",
  "keywords": [
    "discordx",
    "discord.ts",
    "discord.js",
    "bot",
    "discord",
    "typescript"
  ],
  "homepage": "https://github.com/oceanroleplay/discord.ts-example#readme",
  "bugs": {
    "url": "https://github.com/oceanroleplay/discord.ts-example/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oceanroleplay/discord.ts-example.git"
  },
  "license": "MIT",
  "author": "Jochem",
  "type": "module",
  "main": "build/main.js",
  "scripts": {
    "build": "tsc",
    "build:changelog": "npx @discordx/changelog --root=src",
    "dev": "node --loader ts-node/esm/transpile-only src/main.ts",
    "start": "nodemon --exec node --loader ts-node/esm/transpile-only src/main.ts",
    "serve": "node build/main.js",
    "prod": "pm2 start build/main.js"
  },
  "dependencies": {
    "@discordx/importer": "^1.1.2",
    "@discordx/koa": "^1.0.9",
    "@discordx/pagination": "^2.0.1",
    "@discordx/utilities": "^4.1.0",
    "@js-joda/core": "^5.2.0",
    "@koa/router": "^10.1.1",
    "discord.js": "^13.6.0",
    "discordx": "^9.5.2",
    "jsdom": "^19.0.0",
    "koa": "^2.13.4",
    "lowdb": "^3.0.0",
    "node-cache": "^5.1.2",
    "node-fetch": "^3.2.3",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@discordx/changelog": "^2.0.8",
    "@types/jsdom": "^16.2.14",
    "@types/koa": "^2.13.4",
    "dotenv": "^16.0.0",
    "nodemon": "^2.0.15",
    "pm2": "^5.2.0",
    "prettier": "^2.6.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  }
}
